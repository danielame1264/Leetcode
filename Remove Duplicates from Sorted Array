class Solution(object):
    def removeDuplicates(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        if not nums: return 0
        if len(nums)==1: return 1
        pivot = 0
        cursor = 1
        while cursor<len(nums):
            if nums[cursor]==nums[pivot]:
                cursor+=1
            else:
                nums[pivot+1], nums[cursor] = nums[cursor], nums[pivot+1]
                cursor+=1
                pivot +=1
        return pivot+1
