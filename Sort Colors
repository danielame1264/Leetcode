public class Solution {
    public void sortColors(int[] A) {
        int len = A.length;
        int[] stat = new int[3];
        for(int i=0;i<len;i++){
            stat[A[i]]++;
        }
        
        for(int i=0;i<stat[0];i++){
            A[i]=0;
        }
        
        for(int i=0;i<stat[1];i++){
            A[stat[0]+i]=1;
        }
        
        for(int i=0;i<stat[2];i++){
            A[stat[1]+stat[0]+i]=2;
        }
        
    }
}
