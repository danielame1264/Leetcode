class Solution(object):
    def maxProduct(self, words):
        """
        :type words: List[str]
        :rtype: int
        """
        m = 0
        size = len(words)
        bitArray= [self.turnIntoBitMap(i) for i in words]
        print bitArray
        for i in range(size):
            for j in range(i+1, size):
                if bitArray[i]&bitArray[j]==0:
                    m = max(m, len(words[i])*len(words[j]))
        return m
        
    
    def turnIntoBitMap(self, word):
        res = 0  
        for c in word:
            res |=(1<<ord(c)-97)
        return res
